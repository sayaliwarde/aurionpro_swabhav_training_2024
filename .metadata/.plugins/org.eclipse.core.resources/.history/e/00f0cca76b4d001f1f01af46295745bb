package com.aurionpro.streams.test;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class Student {
	public static void main(String[] args) {
		List<String> names = Arrays.asList("Sayali","Neha", "Aagya");
		System.out.println("All students:");
		names.stream().forEach((name)-> System.out.println(name));
		
		List<String> ascendingOrder= names.stream().sorted().collect(Collectors.toList());
		ascendingOrder.stream().forEach((name)->System.out.println(name));
		
	}

}
