package com.aurionpro.behavioural.observer.model;

public class EmailNotifier implements INotifiers {

	@Override
	public void notify(Account account) {
		
		System.out.println(account.credit()+"Amount is credited");
		System.out.println("Your new balance is: "+account.getBalance());
		
		System.out.println(account.debit()+"Amount is debited");
		System.out.println("Your new balance is:" +account.getBalance());
	}
		public String sendEmail() {
			return "Notification send through email";
		}
		
		
	}
	


