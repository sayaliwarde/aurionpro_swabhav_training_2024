package com.aurionpro.model;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;
import org.mockito.Mockito;

class StudentTest {
	
	private IStudent studentServices;
	private Student student;
	
	@BeforeEach
	void init() {
		studentServices =Mockito.mock(IStudent.class);
		student=new Student(studentServices);
	}

	@Test
	void testCalculatePercentage() {
		Mockito.when(studentServices.getmarks()).thenReturn((double) 650);
		Mockito.when(studentServices.getnumberOfSubjects()).thenReturn((double) 10);
		
		assertEquals(65.0, student.calculatePercentage());
		
		
	}

}
